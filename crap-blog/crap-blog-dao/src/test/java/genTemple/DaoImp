package cn.crap.blog.dao.imp;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import cn.crap.blog.dao.I_DEMAIN_Dao;
import cn.crap.blog.dao.mapper._DEMAIN_Mapper;
import cn.crap.blog.domain.dao._DEMAIN_;

@Service
public class _DEMAIN_Dao implements I_DEMAIN_Dao{
	
	@Autowired
	private _DEMAIN_Mapper mapper;
	
	@Override
	public List<_DEMAIN_> selectByIds(List<Long> ids) {
		return mapper.selectByIds(ids);
	}

	@Override
	public int save(_DEMAIN_ t) {
		return mapper.save(t);
	}

	@Override
	public int update(_DEMAIN_ t) {
		return mapper.update(t);
	}

	@Override
	public int delete(long id) {
		return mapper.delete(id);
	}

	@Override
	public _DEMAIN_ get(long id) {
		return mapper.get(id);
	}
}
