package cn.crap.blog.service.imp;

import java.util.ArrayList;
import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import cn.crap.blog.dao.imp._DEMAIN_Dao;
import cn.crap.blog.domain.dao._DEMAIN_;
import cn.crap.blog.service.I_DEMAIN_Service;

@Service
public class _DEMAIN_Service implements I_DEMAIN_Service{

	@Autowired
	private _DEMAIN_Dao dao;
	
	@Override
	public List<_DEMAIN_> selectByIds(List<Long> ids) {
		return dao.selectByIds(ids);
	}

	@Override
	public int save(_DEMAIN_ t) {
		return dao.save(t);
	}

	@Override
	public int update(_DEMAIN_ t) {
		return dao.update(t);
	}

	@Override
	public int delete(long id) {
		return dao.delete(id);
	}

	@Override
	public _DEMAIN_ get(long id) {
		return dao.get(id);
	}
}
